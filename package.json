{
  "name": "dep",
  "version": "0.18.1",
  "description": "A little Node.js dependency installer",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "pretest": "standard \"lib/**/*.js\" \"bin/*.js\" \"test/*js\" \"scripts/*.js\"",
    "test": "tap \"test/*.js\" --timeout 600",
    "test-ci": "tap --coverage \"test/*.js\" --timeout 600",
    "postinstall": "opencollective postinstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/depjs/dep.git"
  },
  "bin": {
    "dep": "./bin/dep.js",
    "depjs": "./bin/dep.js"
  },
  "keywords": [
    "npm"
  ],
  "author": "Daijiro Wachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/depjs/dep/issues"
  },
  "homepage": "https://github.com/depjs/dep#readme",
  "bundleDependencies": [
    "fs-extra",
    "gunzip-maybe",
    "node-gyp",
    "npm-package-arg",
    "npm-path",
    "promise-each",
    "request",
    "semver",
    "tar-fs",
    "tar-stream",
    "update-notifier",
    "which",
    "yargs"
  ],
  "dependencies": {
    "fs-extra": "^8.1.0",
    "gunzip-maybe": "^1.4.1",
    "node-gyp": "^6.0.1",
    "npm-package-arg": "^7.0.0",
    "npm-path": "^2.0.4",
    "opencollective": "^1.0.3",
    "promise-each": "^2.2.0",
    "request": "^2.88.0",
    "semver": "^6.3.0",
    "tar-fs": "^2.0.0",
    "tar-stream": "^2.1.0",
    "update-notifier": "^3.0.1",
    "which": "^2.0.2",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "strong-npm-ls": "^1.0.10",
    "tap": "^14.10.2"
  },
  "engine": {
    "node": ">=4.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/dep",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
